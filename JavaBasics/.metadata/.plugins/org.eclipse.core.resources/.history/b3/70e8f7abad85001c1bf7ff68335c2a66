package Exercise;

import javax.swing.text.StyledEditorKit.ForegroundAction;

public class PrimeNo {
	 public static void main(String[] args) {
		
	
	int a = 0;
	for(int i = 2; i <= 50/2; i++ ){
	    
	int no = i;
	 if (no % i == 0) {
      a=1;	break ; 
	 }	
	if (a==0) {
		System.out.println(no);
	}
	}
	 
	 
	} 
	 }

