package Exercise;

public class Divisibal7 {
     public static void main(String[] args) {
	
    	int a = 0;
    	int b = 0;
    	 
    	 
    	 for (int i = 100; i < 200; i++) {
		if (i % 7== 0) {
		
		a = a + i ;
			b++;
			System.out.println("Divisibal no =" + i);
		}
    	 }
		   System.out.println("sum of all no.-" + a);
	  
		   int c= a/b;
		System.out.println(c);
	
}
