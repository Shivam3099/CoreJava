package com.rays.basics;

public class CountCharAt {
public static void main(String[] args) {
	String s ="Shivam patel";
    int s1=0;
    int s2=0;	
    char c='a';
	char d ='s';
for (int i = 0; i < s.length(); i++) {
	if (c==s.charAt(i)) {
	s1++;	
	}

if(d==s.charAt(i)) {
	s2++;
	}
System.out.println("for cha a="+s1);
System.out.println("for cha s="+s2);


}
}
}
